A#include<iostream>
using namespace std;
int main()
{
          int n,i;
          std::cout<<"Enter the value of n:";
          cin>>n;
          int a1=0,a2=1,a3=n;
          cout<<a1;
          cout<<a2;
          for(i=3;i<=n;i++)
          {
                 a3=a1+a2;
                 cout<<a3;
                 a1=a2;
                 a2=a3;
           }
           return 0;
}
